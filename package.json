{
  "name": "gds-swe-supplyally-govwallet",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=20.11.0"
  },
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "bundle": "esbuild src/index.ts --outdir=bundle --sourcemap --bundle --platform=node --target=node20.10.0",
    "clean": "rimraf dist",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "dev": "NODE_ENV=dev nodemon --watch src --watch test --ext ts,json --exec 'pnpm bundle && pnpm start'",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --fix",
    "lint-check": "eslint .",
    "start": "node --enable-source-maps ./bundle/index.js",
    "test": "NODE_ENV=test glob \"src/**/*.test.ts\" \"test/**/*.test.ts\" -c \"tsx --test\""
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.2",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/node": "20.11.24",
    "@types/nodemon": "1.19.6",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "esbuild": "0.19.12",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "glob": "10.3.10",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "tsx": "4.7.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "csv": "^6.3.8",
    "express": "^4.18.3"
  }
}
